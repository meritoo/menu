{
    "name": "meritoo/menu",
    "description": "Build navigation easily, without any efforts. Library that provides tools to build a menu.",
    "license": "MIT",
    "authors": [
        {
            "name": "Meritoo.pl",
            "email": "github@meritoo.pl",
            "homepage": "http://www.meritoo.pl"
        }
    ],
    "require": {
        "meritoo/common-library": "^1.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "infection/infection": "^0.11.4",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.1",
        "phpunit/phpunit": "^8.0",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^3.0"
    },
    "autoload": {
        "psr-4": {
                "Meritoo\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\Test\\Menu\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
